        <div class="page" style="margin-top:0px, display: block, page-break-inside: avoid, -webkit-region-break-inside: avoid;">
            <style type="text/css">
                .tabledata{
                border-collapse:collapse;
                border:1px soild #000;
                }
                .gxtr >th{
                text-align:justify;
                border-top: 1px solid #000;
                border-left: 1px solid #000;
                border-right: 1px solid #000;
                border-bottom: 1px solid #000;
                background-color:#BEBEBE;
                border-collapse:collapse;
                font-size:14px;
                }
                .gxtr >td{
                text-align:justify;
                border-bottom: 1px solid #000;
                border-right: 1px solid #000;
                border-left: 1px solid #000;
                font-size:14px;
                }
                .herdtd{
                word-wrap:break-word;width:22%;font-size:14px;
                }
                .gxdiv{
                border-left:1px solid #000;
                font-size:14px;
                }
		.footer {
 	  	position: running(footer);
   		left: 0;
   		bottom: 0;
   		width: 100%;
   		background-color: red;
   		color: white;
   		text-align: center;
		}
		.last_page {
    			page-break-before: always; 	
		}

		@page {
   			@bottom-right {
       				content: "Please turn over";
   		 	}
		}

		@page last_page {
    			@bottom-right {
        			content: none;
    			}
		}
            </style>
            <table class="tabledata">
               <div>
                    <tr>
                        <div style="text-align: right;">
                            <h5 style="text-align: right; line-height: 100%">
                                <p style style="line-height: 100%">
				<span t-field="doc_id.name_ent"/>
				<span t-field="doc_id.name"/>
                            </h5>
			    <!--<h5 style="text-align: right;">
                                <span t-field="doc_id.name"/>
                            </h5>-->
                        </div>
			<!--<div style="text-align: right;">
