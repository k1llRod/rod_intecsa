                            <tr style="line-height: 18px; border-bottom:1px solid #000;">
                                <td class="text-center" style="border-right:1px solid #000;">DETALLE</td>
                                <th class="text-center" style="border-right:1px solid #000;">CANT.</th>
                                <th class="text-center" style="border-right:1px solid #000;">P.U.</th>
                                <td class="text-center" style="border-right:1px solid #000;">SUBTOTAL</td>
                            </tr>
                        </thead>
                        <tbody class="invoice_tbody">
                            <t t-set="lines"
                               t-value="o.invoice_line_ids.sorted(key=lambda l: (-l.sequence, l.date, l.move_name, -l.id), reverse=True)"/>
